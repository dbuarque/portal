{
    "name": "lupoex-client",
    "version": "3.6.0",
    "description": "Lupoex Client App.",
    "main": "dist/main.js",
    "productName": "Lupoex Client App",
    "repository": {
        "type": "git",
        "url": "git+ssh://git@github.com/istrau2/lupoex-client"
    },
    "author": "Ishai Strauss",
    "engines": {
        "node": ">= 6.0.0"
    },
    "scripts": {
        "start": "nps",
        "test": "nps test",
        "deps": "nps deps"
    },
    "jest": {
        "modulePaths": [
            "<rootDir>/src",
            "<rootDir>/node_modules"
        ],
        "moduleFileExtensions": [
            "js",
            "json"
        ],
        "transform": {
            "^.+\\.jsx?$": "babel-jest"
        },
        "testRegex": "\\.spec\\.(ts|js)x?$",
        "setupFiles": [
            "<rootDir>/test/jest-pretest.js"
        ],
        "testEnvironment": "node",
        "moduleNameMapper": {
            "aurelia-(.*)": "<rootDir>/node_modules/$1"
        },
        "collectCoverage": true,
        "collectCoverageFrom": [
            "src/**/*.{js,ts}",
            "!**/*.spec.{js,ts}",
            "!**/node_modules/**",
            "!**/test/**"
        ],
        "coverageDirectory": "<rootDir>/test/coverage-jest",
        "coverageReporters": [
            "json",
            "lcov",
            "text",
            "html"
        ]
    },
